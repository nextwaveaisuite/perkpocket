{
  "version": 2,
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/admin/?", "dest": "/public/admin/index.html" },
    { "src": "/(privacy-policy|terms-of-service|affiliate-disclosure|cookie-notice)\\.html", "dest": "/public/$1.html" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
}
